#include <iostream>
using namespace std;
class Rectangle {
int width, height;
public:
    void getdata() {
        cout << "Enter width: ";
        cin >> width;
        cout << "Enter height: ";
        cin >> height;
    }
    void calculatearea() {
        cout << "Area = " << width * height << endl;
    }
};
int main() {
    Rectangle r;
    r.getdata();
    r.calculatearea();
}
